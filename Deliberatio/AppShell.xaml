<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="Deliberatio.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:Deliberatio.Views"
	xmlns:local="clr-namespace:Deliberatio">

	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Main"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:MainPage}"
		Route="MainPage" />
	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Apps"
		Icon="iconlistdetail.png"
		ContentTemplate="{DataTemplate views:AppsPage}"
		Route="AppsPage" />
</Shell>
